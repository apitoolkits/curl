{
    "name": "apitoolkits/curl",
    "description" : "Custom PHP Curl library for the Laravel framework - developed by Apitoolkits",
	"version" : "1.0.5",
	"keywords" : [
		"Laravel",
		"Curl",
		"Apitoolkits"
	],
	"homepage" : "http://apitoolkits.com",
	"license" : "MIT",
    "authors": [
        {
            "name": "apitoolkits",
            "email": "webmaster@apitoolkits.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=4.0"
    },
    "autoload": {        
        "psr-4": {
            "Apitoolkits\\Curl\\": "src/"
        }
    },
	"extra" : {
		"laravel" : {
			"providers" : [
				"Apitoolkits\\Curl\\CurlServiceProvider"
			],
			"aliases" : {
				"Curl" : "Apitoolkits\\Curl\\Facades\\Curl"
			}
		}
	},
	"minimum-stability" : "stable"
}
